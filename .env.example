# Environment Variables for ByteForge Frontend
# Copy this file to .env.local and update with your values

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api

# Timeout Configuration (in milliseconds)
# Client timeout: Used for browser requests (default: 30 seconds)
VITE_CLIENT_TIMEOUT=30000
# Server timeout: Used for SSR requests (default: 10 seconds)
VITE_SERVER_TIMEOUT=10000
